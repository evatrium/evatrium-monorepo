{
  "name": "evatrium-monorepo",
  "version": "0.0.0",
  "private": true,
  "_type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/evatrium/evatrium-monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/evatrium/evatrium-monorepo/issues"
  },
  "_": {
    "workspaces": {
      "packages": [
        "packages/*",
        "apps/*"
      ],
      "nohoist": [
        "**/*"
      ]
    },
    "packageManager": "yarn@1.22.10",
    "engines": {
      "node": ">=14.0.0",
      "yarn": ">=1.22.10"
    }
  },
  "packageManager": "pnpm@7.12.2",
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=7.12.2"
  },
  "scripts": {
    "build": "turbo run build",
    "_dev": "turbo run dev --parallel",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "version": "changeset version && pnpm install --no-frozen-lockfile && pnpm run format",
    "release": "turbo run build && changeset publish",
    "_tester": "turbo run test --filter=@evatrium/foobar"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^26.0.24",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-turbo": "^0.0.4",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "jsdom": "^20.0.1",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "snapshot-diff": "^0.10.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.3.0",
    "turbo": "^1.6.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  },
  "dependencies": {
    "react-pwa": "workspace:^2.0.2",
    "workbox-window": "^6.5.4"
  }
}
